{
  "name": "@openkestrel/theme-motion",
  "version": "0.1.0",
  "description": "View Transition based theme motion core/react/css toolkit",
  "type": "module",
  "sideEffects": [
    "./dist/style.css"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "core": [
        "dist/core.d.ts"
      ],
      "react": [
        "dist/react.d.ts"
      ],
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js",
      "default": "./dist/core.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js",
      "default": "./dist/react.js"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json && cp src/style.css dist/style.css",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.14",
    "typescript": "^5.9.2"
  }
}
